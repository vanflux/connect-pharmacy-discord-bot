{
  "name": "connect-pharmacy-discord-bot",
  "version": "1.0.0",
  "description": "A discord bot for connect pharmacy server",
  "scripts": {
    "start": "yarn dev",
    "dev": "ts-node-dev --exit-child --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "up:db": "docker-compose up -d db phpmyadmin",
    "down:db": "docker-compose stop -d db phpmyadmin",
    "up:wa": "docker-compose up -d owa",
    "down:wa": "docker-compose stop -d owa"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanflux/connect-pharmacy-discord-bot.git"
  },
  "author": "vanflux",
  "bugs": {
    "url": "https://github.com/vanflux/connect-pharmacy-discord-bot/issues"
  },
  "homepage": "https://github.com/vanflux/connect-pharmacy-discord-bot#readme",
  "dependencies": {
    "@open-wa/wa-automate": "^4.58.0",
    "@types/express": "^4.17.17",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.14.6",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "knex": "^2.4.2",
    "mime-types": "^2.1.35",
    "mysql2": "^3.2.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "packageManager": "^yarn@1.20.0+"
}
